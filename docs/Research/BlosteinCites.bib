	author = {Bhaumik, D. and Salzman, E. and Davis, E. and Blostein, F. and Li, G. and Neiswanger, K. and Weyant, R. J. and Crout, R. and McNeil, D. W. and Marazita, M. L. and Foxman, B.},
	booktitle = {JDR Clinical \& Translational Research},
