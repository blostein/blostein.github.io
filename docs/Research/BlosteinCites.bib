	keywords = {Cystitis; Enterococcus; Escherichia coli; Microbiome; Staphylococcus; Streptococcus},
	mesh = {Adult; Aged; Bacterial Infections; Catheter-Related Infections; Catheters, Indwelling; Female; Humans; Middle Aged; Urinary Catheterization; Urinary Catheters; Urinary Tract Infections},
