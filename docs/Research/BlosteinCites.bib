	keywords = {NHANES; cariogenic agent; dental decay; diet; dietary sugars},
	mesh = {Adolescent; Adult; Aged; Aged, 80 and over; Cross-Sectional Studies; DMF Index; Dental Caries; Diet; Humans; Japan; Male; Nutrition Surveys; Prevalence; Prospective Studies; United States; Young Adult},
