	keywords = {Host-microbial interactions; Microbiota; Molecular epidemiology; Premature birth},
	mesh = {Adult; Bacteria; Case-Control Studies; DNA, Bacterial; Female; Gestational Age; Humans; Infant, Newborn; Lactobacillus; Male; Microbiota; Peru; Pregnancy; Premature Birth; RNA, Ribosomal, 16S; Sequence Analysis, DNA; Vagina},
